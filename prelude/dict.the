(b: Block) key: v := {
    find = { l |
        l match: {
            [] -> @nothing
            (e . es) ->
                if: ((e name == @->) && (e values head evaluate == v))
                    then: { @(ok: (e values (at: 1) evaluate)) }
                    else: { find call: [es] }
        }
    }

    find call: [b contents]
} call

v case-of: (b: Block) := b key: v
